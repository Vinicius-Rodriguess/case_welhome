datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

enum Status {
  active
  inactive
}

model Imovel {
  id      Int    @id @default(autoincrement())
  title   String
  address String
  status  Status @default(active)
}